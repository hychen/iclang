#!/usr/bin/env lsc
require! program: commander

program
  .version (require require.resolve \../package.json).version
  .description 'icu is a set of tools for managing iclang runtime.'
  .command 'start-process', 'start a process'
  .command 'stop-process', 'stop a process'
  .command 'run-process', 'run a process'
  .command 'pause-process', 'pause a process'
  .command 'list-process', 'list running processes'
  .command 'connect-process', 'connect a process port to another port.'

program.parse process.argv